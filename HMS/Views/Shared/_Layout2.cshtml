@model HMS.Models.vw_genlay
@{ 
    HMS.Models.worksess work
        = (anchor1.Models.sideview)Session["sidedata"];
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Hotel Management System</title>
    @Scripts.Render("~/bundles/jquerybundle")
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/datatables/bundles/cssbundle")
    @Scripts.Render("~/dataTables/bundles/js1bundle")

    <link href='http://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css' />
</head>
<body>
    <div id="wrapper">
        <nav class="navbar navbar-default navbar-cls-top " role="navigation" style="margin-bottom: 0">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".sidebar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="@works">@Html.Pame</a>
</div>
            <div style="color: white;
padding: 15px 50px 5px 50px;
float: right;
font-size: 16px;">Last access : @worksess.processing_period &nbsp;
                <form method="post" action="@Url.Action("LogOff", "home")" id="signoutfrm" style="display:none;">
                    @Html.AntiForgeryToken()
                </form>
                    <a href="#" onclick="$('#signoutfrm').submit(); return false;" class="btn btn-danger square-btn-adjust">Logout</a>
                </div>
        </nav>
        <!-- /. NAV TOP  -->
        <nav class="navbar-default navbar-side" role="navigation">
                    @RenderBody()
        </nav>
        <div id="page-wrapper">
            <div id="page-inner">
                @DateTime.Now.Year
            </div>
        </div>
        <hr />
        <footer>
            <p>&copy; @Scripts.Render("~/bundles/jsuserbundle") - Bloyd Intelligi</p>
        </footer>
    </div>
    <script type="text/javascript">
        $(function () { // will trigger when the document is r  eady
            $('.datepicker').datepicker({
                changeMonth: true,
                changeYear: true,
                dateFormat: "dd/mm/yy"
            }); //Initialise any date pickers
        });
    </script>
    @RenderSection("scripts", required: false)
    @RenderSection("scripts", required: false)
</body>
</html>
